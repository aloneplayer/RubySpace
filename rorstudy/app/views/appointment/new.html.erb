<% @title="Create new appointment" %> 
<h2><%= @title%></h2>
<%= form_for @appointment, :url => {:action => 'create'} do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <div class="formbuttons">
    <%= f.submit "Create" %>
    </div>
<% end %>
<br/>
<%= link_to 'Back', :controller=>"home", :action => "index" %>
<% content_for :js do %> 
  <%= javascript_include_tag 'new' %> 
<% end %> 
